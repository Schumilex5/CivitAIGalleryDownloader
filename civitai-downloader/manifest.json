{
  "manifest_version": 3,
  "name": "Civitai Fast Auto-Downloader",
  "description": "Automatically scrolls and downloads all images from a Civitai model gallery.",
  "version": "1.2",
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Civitai Downloader",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
