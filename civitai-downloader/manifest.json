{
  "manifest_version": 3,
  "name": "Civitai Fast Downloader",
  "version": "1.0",
  "description": "Auto-scrolls and downloads all images from the current Civitai model gallery quickly.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://civitai.com/*"],
  "icons": { "128": "icon.png" },
  "action": {
    "default_title": "Download all gallery images"
  },
  "background": {
    "service_worker": "background.js"
  }
}
